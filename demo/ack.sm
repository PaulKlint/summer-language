proc ackermann(m,n)
(
  if m = 0 then
     return(n + 1)
  elif n = 0 then
     return(ackermann(m-1,1))
  else
     return(ackermann(m-1, ackermann(m,n-1)))
  fi
);

program main()
( var i;

  for i in interval(0, 3, 1)
  do var j;
     for j in interval(0, 5, 1)
     do
        put(i, '  ', j, '  ', ackermann(i,j), '\n')
     od
  od
)
