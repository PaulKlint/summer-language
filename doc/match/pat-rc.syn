<pattern> ::= <pattern-primary> [ ( '|' | '--' ) <pattern-primary> ] .
.sp 0.5
<pattern-primary> ::=
        '(' <pattern> ')' |
        <string-literal> |
        <identifier> |
        '*' <identifier> |
        SBAS '(' <pattern-primary> ',' <identifier> ')' |
        ACT  '(' <expression> ')' |
        TRY <window> {<pattern> ','}+ UNTIL <pattern> YRT .
.sp 0.5
<window> ::=  '<' { <identifier> ',' }* '>' .
