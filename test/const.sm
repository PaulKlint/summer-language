proc error(m)	put('ERROR ', m, '\n');

const x := 'x1',
      y := 'y1';

class C()
begin	const x := 'x2';

	proc p()
	(	if x ~= 'x2'
		then	error(1)
		fi;
	);

	init:	const x := 'x3';
		p();
		if x ~= 'x3'
		then	error(2)
		fi;
end C;

const a := 2, b := -3, c := 2 * a + b;

program con()
(	if y ~= 'y1' then error(3) fi;
	(	const y := 'y2';

		if y ~= 'y2' then error(4) fi;
		(	const y := 'y4';
			if y ~= 'y4' then error(5) fi
		);
		if y ~= 'y2' then error(6) fi
	);
	if y ~= 'y1' then error(7) fi;
	case 'y1' of
		x : y: ,
		default: error(8)
	esac;

	if a ~= 2 | b ~= -3 | c ~= 1 then error(9) fi;

)
